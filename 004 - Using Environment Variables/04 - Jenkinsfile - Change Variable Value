pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	environment{
        NUMBER_OF_ISSUES = 0 
    }
	
    stages {
		stage('teste'){
			steps{
				env.NUMBER_OF_ISSUES = 10
			}
		}
		stage('notification'){
			steps{
				echo env.NUMBER_OF_ISSUES
			}
		}	
    }
}