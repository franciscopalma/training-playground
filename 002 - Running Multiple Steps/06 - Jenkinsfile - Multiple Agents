pipeline {
    agent none
    stages {
		stage('maven'){
			agent {
				docker { 
    				image 'maven:3.3.3' 
					} 
				}
			steps{
				sh 'mvm --version'
			}
		}
		stage('npm'){
			agent {
				docker { 
    				image 'node:6.3' 
					} 
				}
			steps{
				sh 'npm --version'
			}
		}
    }
}